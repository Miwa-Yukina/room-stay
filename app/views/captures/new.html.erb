<% provide(:title, 'Capture') %>
<h1>Index</h1>

<table border="1">
  <tr>
    <th>MACアドレス</th> <th>RSSI</th> <th>SNR</th> <th>受信日時</th>
  </tr>
  <% @captures.each do |capture| %>
    <tr>
      <td><%= capture.capture %></td>
      <td align="right"><%= capture.rssi %>dBm</td>
      <td align="right"><%= capture.snr %></td>
      <td><%= capture.created_at %></td>
    </tr>
  <% end %>
</table>
